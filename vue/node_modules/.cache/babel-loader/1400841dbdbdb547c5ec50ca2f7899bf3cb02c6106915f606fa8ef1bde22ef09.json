 {\r\n        if (res.code === '200') {\r\n          this.tableData = res.data;\r\n          this.total = res.total;\r\n        }\r\n      });\r\n\r\n\r\n    },\r\n\r\n    // 显示选课学生\r\n    showStudents(row) {\r\n      this.selectedStudents = row.students;  // 设置选课学生数据\r\n      this.studentsDialogVisible = true;  // 显示弹框\r\n    },\r\n\r\n    // 页码变化时加载数据\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum;\r\n      this.load(pageNum);\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AA6CA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,gBAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,qBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACA;IACAD,KAAAV,OAAA;MACA;MACA,KAAAY,QAAA,CAAAC,GAAA;QAAAC,MAAA;UAAAd,OAAA;UAAAC,QAAA,OAAAA;QAAA;MAAA,GAAAc,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAnB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA;UACA,KAAAK,KAAA,GAAAc,GAAA,CAAAd,KAAA;QACA;MACA;IAGA;IAEA;IACAgB,aAAAC,GAAA;MACA,KAAApB,gBAAA,GAAAoB,GAAA,CAAAC,QAAA;MACA,KAAAZ,qBAAA;IACA;IAEA;IACAa,oBAAArB,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,IAAA,CAAAV,OAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}