{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Manager\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\")\n    };\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push(\"/login\");\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"); // 重新获取下用户的最新信息\n    },\n    goToPerson() {\n      if (this.user.role === \"ADMIN\") {\n        this.$router.push(\"/adminPerson\");\n      }\n      if (this.user.role === \"TEACHER\") {\n        this.$router.push(\"/teacherPerson\");\n      }\n      if (this.user.role === \"STUDENT\") {\n        this.$router.push(\"/studentPerson\");\n      }\n    },\n    logout() {\n      localStorage.removeItem(\"xm-user\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","created","id","$router","push","methods","updateUser","goToPerson","role","logout","removeItem"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\r\n  <div class=\"manager-container\">\r\n    <!-- 头部 -->\r\n    <div class=\"manager-header\">\r\n      <div class=\"manager-header-left\">\r\n        <img src=\"@/assets/imgs/logo.png\" alt=\"Logo\" />\r\n        <div class=\"title\">教务管理系统</div>\r\n      </div>\r\n\r\n      <div class=\"manager-header-center\">\r\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\r\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n      </div>\r\n\r\n      <div class=\"manager-header-right\">\r\n        <el-dropdown placement=\"bottom\">\r\n          <div class=\"avatar\">\r\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\r\n            <div>{{ user.name || '管理员' }}</div>\r\n          </div>\r\n          <el-dropdown-menu slot=\"dropdown\">\r\n            <el-dropdown-item @click.native=\"goToPerson\">\r\n              <i class=\"el-icon-user\"></i> 个人信息\r\n            </el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"$router.push('/password')\">\r\n              <i class=\"el-icon-lock\"></i> 修改密码\r\n            </el-dropdown-item>\r\n            <el-dropdown-item @click.native=\"logout\">\r\n              <i class=\"el-icon-switch-button\"></i> 退出登录\r\n            </el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </el-dropdown>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主体 -->\r\n    <div class=\"manager-main\">\r\n      <!-- 侧边栏 -->\r\n      <div class=\"manager-main-left\">\r\n        <el-menu router style=\"border: none\" :default-active=\"$route.path\" class=\"menu\">\r\n    \r\n          <el-submenu index=\"info\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-notebook-1\"></i><span>信息公告</span>\r\n            </template>\r\n            <el-menu-item index=\"/notice\" v-if=\"user.role === 'ADMIN'\">\r\n              <i class=\"el-icon-document\"></i> 教务通知\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/examPlan\" v-if=\"user.role === 'ADMIN'\">\r\n              <i class=\"el-icon-date\"></i> 考试安排\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/roomPlan\">\r\n              <i class=\"el-icon-office-building\"></i> 教室安排\r\n            </el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"administration\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-s-management\"></i><span>行政管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/college\">\r\n              <i class=\"el-icon-school\"></i> 学院信息\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/speciality\">\r\n              <i class=\"el-icon-s-flag\"></i> 专业信息\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/classes\">\r\n              <i class=\"el-icon-s-grid\"></i> 班级信息\r\n            </el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"teach\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-s-operation\"></i><span>教学管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/course\">\r\n              <i class=\"el-icon-reading\"></i> 课程信息\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/choice\">\r\n              <i class=\"el-icon-notebook-2\"></i> 我的选课\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/curriculum\" v-if=\"user.role === 'STUDENT'\">\r\n              <i class=\"el-icon-tickets\"></i> 我的课表\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/score\">\r\n              <i class=\"el-icon-medal\"></i> 我的成绩\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/comment\">\r\n              <i class=\"el-icon-chat-line-round\"></i> 网上评教\r\n            </el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"educational\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-s-order\"></i><span>教务管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/apply\" v-if=\"user.role !== 'TEACHER'\">\r\n              <i class=\"el-icon-edit-outline\"></i> 请假申请\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/homework\">\r\n              <i class=\"el-icon-folder\"></i> 作业提交\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/attendance\">\r\n              <i class=\"el-icon-document-checked\"></i> 考勤信息\r\n            </el-menu-item>\r\n          </el-submenu>\r\n          <el-submenu index=\"user\" v-if=\"user.role === 'ADMIN'\">\r\n            <template slot=\"title\">\r\n              <i class=\"el-icon-user-solid\"></i><span>用户管理</span>\r\n            </template>\r\n            <el-menu-item index=\"/admin\">\r\n              <i class=\"el-icon-s-custom\"></i> 管理员信息\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/teacher\">\r\n              <i class=\"el-icon-s-cooperation\"></i> 教师信息\r\n            </el-menu-item>\r\n            <el-menu-item index=\"/student\">\r\n              <i class=\"el-icon-s-check\"></i> 学生信息\r\n            </el-menu-item>\r\n          </el-submenu>\r\n        </el-menu>\r\n      </div>\r\n\r\n      <!-- 数据表格 -->\r\n      <div class=\"manager-main-right\">\r\n        <router-view @update:user=\"updateUser\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Manager\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\r\n    };\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$router.push(\"/login\");\r\n    }\r\n  },\r\n  methods: {\r\n    updateUser() {\r\n      this.user = JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"); // 重新获取下用户的最新信息\r\n    },\r\n    goToPerson() {\r\n      if (this.user.role === \"ADMIN\") {\r\n        this.$router.push(\"/adminPerson\");\r\n      }\r\n      if (this.user.role === \"TEACHER\") {\r\n        this.$router.push(\"/teacherPerson\");\r\n      }\r\n      if (this.user.role === \"STUDENT\") {\r\n        this.$router.push(\"/studentPerson\");\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"xm-user\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.manager-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100vh;\r\n}\r\n\r\n.manager-header {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 10px 20px;\r\n  background: white;\r\n  color: white;\r\n}\r\n\r\n.manager-header-left {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.manager-header-left img {\r\n  height: 40px;\r\n  margin-right: 10px;\r\n}\r\n\r\n.manager-header-left .title {\r\n  font-size: 18px;\r\n  font-weight: bold;\r\n}\r\n\r\n.manager-header-right {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.manager-header-right .avatar {\r\n  display: flex;\r\n  align-items: center;\r\n  cursor: pointer;\r\n}\r\n\r\n.manager-header-right .avatar img {\r\n  height: 30px;\r\n  width: 30px;\r\n  border-radius: 50%;\r\n  margin-right: 5px;\r\n}\r\n\r\n.manager-main {\r\n  display: flex;\r\n  height: calc(100vh - 60px);\r\n}\r\n\r\n.manager-main-left {\r\n  width: 240px;\r\n  background: white;\r\n  color: white;\r\n}\r\n\r\n.manager-main-right {\r\n  flex: 1;\r\n  padding: 20px;\r\n  background: #f4f5f7;\r\n}\r\n\r\n.el-menu .el-menu-item,\r\n.el-submenu .el-submenu__title {\r\n  font-size: 14px;\r\n}\r\n\r\n.el-menu .el-menu-item i,\r\n.el-submenu .el-submenu__title i {\r\n  margin-right: 5px;\r\n}\r\n\r\n.el-menu .menu {\r\n  font-size: 14px;\r\n}\r\n</style>\r\n"],"mappings":";AAmIA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,UAAAL,IAAA,CAAAM,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAV,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;IACAO,WAAA;MACA,SAAAX,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;MACA,SAAAR,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;MACA,SAAAR,IAAA,CAAAY,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;IACA;IACAK,OAAA;MACAV,YAAA,CAAAW,UAAA;MACA,KAAAP,OAAA,CAAAC,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}