olumn\r\n            type=\"selection\"\r\n            width=\"55\"\r\n            align=\"center\"\r\n            v-if=\"user.role === 'GUANLIYUAN'\"\r\n        ></el-table-column>\r\n        <el-table-column\r\n            prop=\"id\"\r\n            label=\"序号\"\r\n            width=\"80\"\r\n            align=\"center\"\r\n            sortable\r\n        ></el-table-column>\r\n        <el-table-column\r\n            prop=\"name\"\r\n            label=\"学院名称\"\r\n            show-overflow-tooltip\r\n        ></el-table-column>\r\n        <el-table-column\r\n            prop=\"neirong\"\r\n            label=\"学院描述\"\r\n            show-overflow-tooltip\r\n        ></el-table-column>\r\n        <el-table-column\r\n            label=\"操作\"\r\n            width=\"180\"\r\n            align=\"center\"\r\n            v-if=\"user.role === 'GUANLIYUAN'\"\r\n        >\r\n          <template v-slot=\"scope\">\r\n            <el-button\r\n                plain\r\n                type=\"primary\"\r\n                @click=\"handleEdit(scope.row)\"\r\n                size=\"mini\"\r\n            >\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n                plain\r\n                type=\"danger\"\r\n                size=\"mini\"\r\n                @click=\"del(scope.row.id)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <div class=\"pagination\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"College\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      name: null,\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    };\r\n  },\r\n  created() {\r\n    this.load(1);\r\n  },\r\n  methods: {\r\n    load(pageNum) {\r\n      if (pageNum) this.pageNum = pageNum;\r\n      this.$request.get('/xueyuan/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list;\r\n        this.total = res.data?.total;\r\n      });\r\n    },\r\n    reset() {\r\n      this.name = null;\r\n      this.load(1);\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum);\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.xueyuan-container {\r\n  background-color: #f9f9f9;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n}\r\n\r\n.search {\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.search-input {\r\n  width: 200px;\r\n  border-radius: 5px;\r\n}\r\n\r\n.action-btn {\r\n  border-radius: 5px;\r\n  padding: 6px 12px;\r\n}\r\n\r\n.table .custom-table {\r\n  background-color: #fff;\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\r\n  border-radius: 8px;\r\n}\r\n\r\n.pagination {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-neirong: center;\r\n  padding: 10px;\r\n}\r\n\r\n.el-button {\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.el-button:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n\r\n.el-pagination {\r\n  font-size: 14px;\r\n  padding: 10px;\r\n}\r\n\r\n.el-table th, .el-table td {\r\n  font-size: 14px;\r\n  padding: 10px;\r\n}\r\n\r\n.el-table .el-table__body {\r\n  font-size: 13px;\r\n}\r\n\r\n.el-table-column {\r\n  min-width: 120px;\r\n}\r\n\r\n.el-table__cell {\r\n  word-break: break-word;\r\n}\r\n</style>\r\n"],"mappings":"AAsGA;EACAA,IAAA;E