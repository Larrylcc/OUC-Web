知和考试安排并排展示 -->\r\n    <div style=\"display: flex; margin-bottom: 20px;\">\r\n      <!-- 教务通知 -->\r\n      <div style=\"width: 50%; margin-right: 10px; padding: 15px;\" class=\"card\">\r\n        <div style=\"margin-bottom: 20px; font-size: 18px; font-weight: bold\">教务通知</div>\r\n        <el-timeline reverse>\r\n          <el-timeline-item\r\n              v-for=\"item in notices\"\r\n              :key=\"item.id\"\r\n              :timestamp=\"item.time\">\r\n            <el-popover\r\n                placement=\"right\"\r\n                width=\"200\"\r\n                trigger=\"hover\"\r\n                :content=\"item.content\">\r\n              <span slot=\"reference\">{{ item.title }}</span>\r\n            </el-popover>\r\n          </el-timeline-item>\r\n        </el-timeline>\r\n      </div>\r\n\r\n      <!-- 考试安排 -->\r\n      <div style=\"width: 50%; margin-left: 10px; padding: 15px;\" class=\"card\">\r\n        <div style=\"margin-bottom: 20px; font-size: 18px; font-weight: bold\">考试安排</div>\r\n        <el-timeline reverse>\r\n          <el-timeline-item\r\n              v-for=\"item in examplans\"\r\n              :key=\"item.id\"\r\n              :timestamp=\"item.time\">\r\n            <el-popover\r\n                placement=\"right\"\r\n                width=\"200\"\r\n                trigger=\"hover\"\r\n                :content=\"item.content\">\r\n              <span slot=\"reference\">{{ item.name }}</span>\r\n            </el-popover>\r\n          </el-timeline-item>\r\n        </el-timeline>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 课表展示 -->\r\n    <div style=\"padding: 15px;\" class=\"card\">\r\n      <div style=\"margin-bottom: 30px; font-size: 20px; font-weight: bold\">课表</div>\r\n      <el-table :data=\"tableData\" stripe border>\r\n        <el-table-column prop=\"segment\" label=\"时间\" width=\"200\"></el-table-column>\r\n        <el-table-column\r\n            v-for=\"day in days\"\r\n            :key=\"day\"\r\n            :prop=\"day.prop\"\r\n            :label=\"day.label\"\r\n            show-overflow-tooltip>\r\n          <template #default=\"scope\">\r\n            <div\r\n                v-if=\"scope.row[day.prop]\"\r\n                style=\"background-color: #e6f7ff; color: #1890ff; text-align: center; border-radius: 4px; padding: 4px;\">\r\n              {{ scope.row[day.prop] }}\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || \"{}\"),\r\n      notices: [],\r\n      examplans: [],\r\n      tableData: [],\r\n      days: [\r\n        { label: \"星期一\", prop: \"monday\" },\r\n        { label: \"星期二\", prop: \"tuesday\" },\r\n        { label: \"星期三\", prop: \"wednesday\" },\r\n        { label: \"星期四\", prop: \"thursday\" },\r\n        { label: \"星期五\", prop: \"friday\" },\r\n        { label: \"星期六\", prop: \"saturday\" },\r\n        { label: \"星期日\", prop: \"sunday\" },\r\n      ],\r\n    };\r\n  },\r\n  created() {\r\n    this.loadNotices();\r\n    this.loadExamPlans();\r\n    this.loadCurriculum();\r\n  },\r\n  methods: {\r\n    // 加载教务通知\r\n    loadNotices() {\r\n      this.$request.get(\"/notice/selectAll\").then((res) => {\r\n        if (res.code === \"200\") {\r\n          this.notices = res.data || [];\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      });\r\n    },\r\n    // 加载考试安排\r\n    loadExamPlans() {\r\n      this.$request.get(\"/examplan/selectAll\").then((res) => {\r\n        if (res.code === \"200\") {\r\n          this.examplans = res.data || [];\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      });\r\n    },\r\n    // 加载课表\r\n    loadCurriculum() {\r\n      this.$request.get(\"/choice/getCurriculum\").then((res) => {\r\n        if (res.code === \"200\") {\r\n          this.tableData = res.data || [];\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card {\r\n  background-color: #fff;\r\n  border: 1px solid #ebeef5;\r\n  border-radius: 6px;\r\n  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n</style>\r\n"],"mappings":"AAoEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,OAAA;MACAC,SAAA;MACAC,SAAA;MACAC,IAAA,GACA;QAAAC,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,KAAA;QAAAC,IAAA;MAAA;IAEA;EACA;EACAC,QAAA;IACA,KAAAC,WAAA;IACA,KAAAC,aAAA;IACA,KAAAC,cAAA;EACA;EACAC,OAAA;IACA;IACAH,YAAA;MACA,KAAAI,QAAA,CAAAC,GAAA,sBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAf,OAAA,GAAAc,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACA;IACAV,cAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAd,SAAA,GAAAa,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACA;IACAT,eAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,0BAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAb,SAAA,GAAAY,GAAA,CAAApB,IAAA;QACA;UACA,KAAAsB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}